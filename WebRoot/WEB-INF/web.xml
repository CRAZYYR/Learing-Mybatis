<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>ListServlet</servlet-name>
  	<servlet-class>com.mylzs.servlet.ListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>ListServlet</servlet-name>
  <url-pattern>/List.action</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>deleteOne</servlet-name>
  	<servlet-class>com.mylzs.servlet.DeleteOne</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>deleteOne</servlet-name>
  	<url-pattern>/delete.action</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>InitTalk</servlet-name>
  	<servlet-class>com.mylzs.servlet.InitTalk</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>InitTalk</servlet-name>
  	<url-pattern>/InitTalk.action</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>autoreply</servlet-name>
  	<servlet-class>com.mylzs.servlet.AutoReply</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>autoreply</servlet-name>
  	<url-pattern>/AutoReply.action</url-pattern>
  </servlet-mapping>
</web-app>
